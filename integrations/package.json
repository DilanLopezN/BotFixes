{
  "name": "kissbot-integrations",
  "version": "0.0.1",
  "description": "Integration services for Kissbot platform",
  "author": "Kissbot",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "audit-check": "chmod 766 ./bin/audit.sh && ./bin/audit.sh",
    "prebuild": "rimraf dist",
    "prepare": "husky && chmod ug+x .husky/*",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "new-integration": "node ./scripts/integration.js",
    "prestart:prod": "rimraf dist && tsc && tsc-alias",
    "start": "nest start",
    "start:dev": "export NODE_ENV=local && nest start --watch --debug 0.0.0.0:9231",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest --detectOpenHandles --forceExit --coverage ",
    "test:dev": "docker exec -it bd__integrations npm run test",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest ./test/jest-e2e.json",
    "console": "node dist/console.js",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.803.0",
    "@aws-sdk/client-s3": "^3.803.0",
    "@aws-sdk/client-secrets-manager": "^3.803.0",
    "@aws-sdk/credential-providers": "^3.803.0",
    "@aws-sdk/lib-dynamodb": "^3.803.0",
    "@aws-sdk/s3-request-presigner": "^3.803.0",
    "@golevelup/nestjs-rabbitmq": "^5.6.1",
    "@google/generative-ai": "^0.21.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.0",
    "@sentry/node": "^7.5.1",
    "axios": "^1.6.7",
    "axios-retry": "^4.5.0",
    "chance": "^1.1.11",
    "class-transformer": "0.3.1",
    "class-validator": "^0.14.1",
    "express": "^4.18.2",
    "fast-fuzzy": "^1.12.0",
    "file-type": "^16.5.4",
    "form-data": "^3.0.0",
    "heic-convert": "^2.1.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.2",
    "kissbot-core": "git+https://deploy-core:gldt-ebyZhGsLHTp-ZEaEtmmF@gitlab.com/kissbot/kissbot-core.git",
    "kissbot-entities": "git+https://kissbot-entities-deploy:Q2xro3rFKCx2fTr8F5GF@gitlab.com/kissbot/kissbot-entities.git",
    "kissbot-health-core": "git+https://kissbot-health-core:Qfq-hyGCr-vTyc9ZE6A_@gitlab.com/kissbot/kissbot-health-core.git",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.7.0",
    "nanoid": "^3.3.6",
    "openai": "^4.104.0",
    "pg": "^8.8.0",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rtf-parser": "^1.3.3",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.34.3",
    "soap": "^1.0.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@types/chance": "^1.1.3",
    "@types/compression": "^1.7.5",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.21",
    "@types/heic-convert": "^2.1.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.166",
    "@types/md5": "^2.2.1",
    "@types/node": "22.5.4",
    "@types/sanitize-html": "^2.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.26.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text"
    ],
    "collectCoverageFrom": [
      "**/api-queue*.(j|t)s",
      "!**/*.spec.(j|t)s"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "testEnvironment": "node"
  }
}
