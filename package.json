{
  "name": "kissbot-api",
  "version": "0.0.1",
  "description": "",
  "author": "Kissbot TEAM",
  "license": "MIT",
  "scripts": {
    "audit-check": "chmod 766 ./bin/audit.sh && ./bin/audit.sh",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint --fix .",
    "build": "rm -rf dist && tsc --skipLibCheck; tscpaths -p tsconfig.json -s src -o dist",
    "prestart:prod": "rm -rf dist && tsc -v && tsc; tscpaths -p tsconfig.json -s src -o dist",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/modules/database-migrations/config/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "start": "node dist/main.js",
    "start:dev": "export NODE_ENV=local && nest start --watch --debug 0.0.0.0:9232",
    "start:http0": "export NODE_INDEX=0 && export PORT=9092 && npm run start:dev",
    "start:http1": "export NODE_INDEX=1 && export PORT=9093 && npm run start:dev",
    "test": "export NODE_ENV=test && jest --forceExit --runInBand",
    "test:dev": "export NODE_ENV=test && export MONGO_URI=mongodb://localhost:27017/kissbot-api-tests && jest --forceExit --runInBand",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
